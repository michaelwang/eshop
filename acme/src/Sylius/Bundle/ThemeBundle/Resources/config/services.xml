<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
      <service id="theme_reviews" class="Sylius\Bundle\ThemeBundle\Form\Type\ProductReviewType">
             <argument>Sylius\Bundle\ThemeBundle\Form\Type\ProductReviewType</argument>
             <tag name="form.type" alias="theme_reviews" />
      </service>
      <service id="pagerfanta.view.homeshop_view" class="Sylius\Bundle\ThemeBundle\View\HomeshopView" public="false">
            <tag name="pagerfanta.view" alias="homeshop_view" />
        </service>
    </services>
</container>
